# GPM API Configuration
GPM_API_BASE_URL=http://127.0.0.1:12003/api/v3
GPM_API_TIMEOUT=30

# Profile Storage Directory
# Leave empty to use default: %USERPROFILE%/profiles
GPM_PROFILES_DIR=

# Browser Window Settings
BROWSER_WIDTH=1000
BROWSER_HEIGHT=700
BROWSER_SCALE=0.8
MAX_BROWSERS_PER_LINE=4

# Retry Configuration
MAX_RETRIES=3
RETRY_DELAY=5
CONNECTION_WAIT_TIME=3

# CPU Detection Settings
CPU_THRESHOLD=2.0
CPU_CHECK_INTERVAL=1.5

# Debug Mode
DEBUG=false
